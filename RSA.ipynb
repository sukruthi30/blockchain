        "import math\n"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "message = int(input(\"enter the message to be encrypted:\"))\n",
        "p=11\n",
        "q=7\n",
        "e=3\n",
        "n=p*q\n",
        "def encrypt(me):\n",
        "  en=math.pow(me,e)\n",
        "  c=en%n\n",
        "  print(\"encrypted message: \",c)\n",
        "  return c\n",
        "print(\"original message: \",message)\n",
        "c=encrypt(message)\n"
      ],
